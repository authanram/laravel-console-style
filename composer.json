{
    "name": "authanram/laravel-console-style",
    "type": "package",
    "description": "...",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.17",
        "nunomaduro/phpinsights": "^2.1",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-mock": "^1.0",
        "spatie/ray": "^1.33"
    },
    "autoload": {
        "psr-4": {
            "Authanram\\LaravelConsoleStyle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Authanram\\LaravelConsoleStyle\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
