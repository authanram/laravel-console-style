{
    "name": "authanram/laravel-console-style",
    "type": "package",
    "description": "...",
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Authanram\\LaravelConsoleStyle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Authanram\\LaravelConsoleStyle\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
